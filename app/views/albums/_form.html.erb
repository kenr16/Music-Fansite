<%= render "layouts/errors", :object => @album %>

<%= form_for @album do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :artist %>
  <%= f.text_field :artist %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.submit  %>
<% end %>
